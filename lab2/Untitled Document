#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <time.h>
#include <string.h>
#DEFINE OCUPADO 1
#DEFINE LIVRE 0

typedef struct{
  int estado;
}canal;

typedef struct{
  int canais_ocupados;
  canal* canais;
}servidor;


int main(void){
  servidor* server;
  server = malloc(sizeof(server));
  double l = 0.009; //taxa de chegada de clientes (1/hora)
  double dm=2; //media do tempo de serviÃ§o
  int m=8; //numero de canais
  server->canais = malloc(sizeof(canal)*m);
  for(int i=0;i<m;i++){
     server->canal[i]=LIVRE;
  }
  free(server); 	
}

